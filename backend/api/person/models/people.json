{
  "kind": "collectionType",
  "collectionName": "person",
  "info": {
    "name": "Person",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "biginteger"
    },
    "extension": {
      "type": "integer"
    },
    "photo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "startDate": {
      "type": "date"
    },
    "class": {
      "model": "class",
      "via": "people"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "person"
    },
    "positions": {
      "collection": "position",
      "via": "people"
    },
    "services": {
      "collection": "service",
      "via": "providers"
    }
  }
}
